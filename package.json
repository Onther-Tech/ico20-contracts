{
  "name": "hardhat-project",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "compile:plasma": "bash scripts/build-plasma-evm-contracts.sh",
    "compile:uniswap-core": "bash scripts/build-uniswap-core-contracts.sh",
    "compile:uniswap3-core": "bash scripts/build-uniswap3-core-contracts.sh",
    "compile:uniswap-periphery": "bash scripts/build-uniswap-periphery-contracts.sh",
    "compile:uniswap3-periphery": "bash scripts/build-uniswap3-periphery-contracts.sh",
    "compile:truffle": "rm -rf ./build/contracts && truffle compile && cp ./abis_plasma/* ./build/contracts/ ",
    "test:proxy": "truffle test test/proxy.test.js",
    "test:stake.ton": "truffle test test/stake.ton.test.js",
    "test:stake.tokamak": "truffle test test/stake.ton-to-tokamak.test.js",
    "test:uniswapv3": "npx hardhat test  test/stake-uniswapv3/stake.uniswap3.stake.js",
    "test:upgrade-stakeTON": "truffle test test/upgrade.stake-ton.test.js",
    "test:stake.eth": "truffle test test/stake-eth/stake.eth.test.js",
    "test": "npx hardhat test",
    "test:mocha": "npx mocha --recursive --exit",
    "coverage": "npm run build && npx hardhat coverage --temp artifacts --network coverage",
    "deploy-rinkeby": "npx hardhat run --network rinkeby scripts/deploy.js",
    "solhint": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "lint": "pretty-quick --pattern '**/*.*(sol)' --verbose",
    "lint:check": "prettier --check **/*.sol",
    "lint:fix": "pretty-quick --pattern '**//*.*(sol)' --staged --verbose",
    "lint:write": "prettier --write **/*.sol **/*.json",
    "docify": "node docify.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "3.3.0",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/contracts-upgradeable": "^3.3.0",
    "@openzeppelin/hardhat-upgrades": "^1.6.0",
    "@openzeppelin/test-helpers": "^0.5.9",
    "@typechain/ethers-v5": "^6.0.2",
    "@typechain/hardhat": "^1.0.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.32",
    "buidler-gas-reporter": "^0.1.4",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.2",
    "hardhat": "^2.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-alpha.57",
    "pretty-quick": "^3.1.0",
    "solc-0.7": "npm:solc@^0.7.6",
    "solhint": "^3.3.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3",
    "web3": "^1.3.6"
  },
  "dependencies": {
    "@ethersproject/address": "^5.1.0",
    "@ethersproject/contracts": "^5.1.1",
    "@ethersproject/providers": "^5.1.2",
    "@ethersproject/solidity": "^5.1.0",
    "@makerdao/currency": "^0.9.9",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/truffle-upgrades": "^1.5.0",
    "@uniswap/lib": "4.0.1-alpha",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/v2-core": "1.0.0",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-periphery": "^1.0.1",
    "abi-decoder": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "bignumber.js": "^9.0.1",
    "bn.js": "^5.2.0",
    "chai-bignumber": "^3.0.0",
    "chai-bn": "^0.2.1",
    "eslint": "^7.25.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-standard": "^5.0.0",
    "eth-gas-reporter": "^0.2.22",
    "eth-permit": "^0.1.10",
    "load-json-file": "^1.1.0",
    "openzeppelin-test-helpers": "^0.5.1",
    "solidity-docgen": "^0.5.13",
    "standard": "^16.0.3",
    "web3-eth-abi": "^1.3.5"
  }
}
